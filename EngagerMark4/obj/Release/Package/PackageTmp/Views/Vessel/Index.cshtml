@model EngagerMark4.ApplicationCore.Cris.Configurations.CommonConfigurationCri
@{
    ViewBag.Title = "Vessel";
}

@Html.Partial("_ListMessage")

<div class="panel panel-info">
    <div class="panel-heading">Vessel</div>
    <div class="panel-body">
        <div class="form-horizontal">
            <div class="row">
                <div class="col-lg-3">
                    @Html.TextBoxFor(x => x.SearchValue,new { @class="form-control", @placeholder="Code/Name" })
                </div>
                <div class="col-lg-1">
                    <input type="submit" id="btnSearch" class="btn btn-success" value="Search" />
                </div>
            </div>
        </div>
        <br />
        <div class="form-group">
            <a href="~/Vessel/Details" class="btn btn-lg btn-block btn-primary"><i class="fa fa fa-plus-square"></i>&nbsp &nbsp New Vessel</a>
        </div>

        <div id="result">

        </div>
    </div>
</div>

<script src="~/Scripts/Custom/Vessel/Index.js"></script>